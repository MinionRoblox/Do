local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local TextBox = Instance.new("TextBox")

-- Properties

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.new(1, 1, 1)
Frame.BorderColor3 = Color3.new(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.157342657, 0, 0.149098471, 0)
Frame.Size = UDim2.new(0, 626, 0, 373)

UICorner.Parent = Frame

TextBox.Parent = Frame
TextBox.BackgroundColor3 = Color3.new(0, 0, 0)
TextBox.BorderColor3 = Color3.new(0, 0, 0)
TextBox.BorderSizePixel = 0
TextBox.Position = UDim2.new(0.289137393, 0, 0.321715832, 0)
TextBox.Size = UDim2.new(0, 246, 0, 68)
TextBox.Font = Enum.Font.SourceSans
TextBox.Text = ""
TextBox.TextColor3 = Color3.new(1, 1, 1)
TextBox.TextSize = 14

-- Scripts

local function ETBT_fake_script() -- TextBox.Script 
	local script = Instance.new('Script', TextBox)

	local function kickPlayer(playerName)
		local playerToKick = game.Players:FindFirstChild(playerName)
		if playerToKick then
			playerToKick:Kick("Вы были кикнуты администратором.")
		else
			print("Игрок с таким именем не найден.")
		end
	end
	
end
coroutine.wrap(ETBT_fake_script)()
